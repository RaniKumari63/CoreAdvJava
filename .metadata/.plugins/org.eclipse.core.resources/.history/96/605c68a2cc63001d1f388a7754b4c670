package fundamental;

public class OperatorEx38 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * int a=10; a+=20; System.out.println(a);
		 */


int a=30;
a-=20;
System.out.println(a);


int a=10;
a+=20;
System.out.println(a);

int a=10;
a+=20;
System.out.println(a);
	}

}

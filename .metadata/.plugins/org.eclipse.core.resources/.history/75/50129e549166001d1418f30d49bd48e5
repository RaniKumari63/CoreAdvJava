package com.iterate;

import java.util.Scanner;

public class ForEx3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i=0;i<2;++i) //rows
		{   
			for(int j=0;j<6;++j) //columns
			{
				System.out.print("A");
			}
			System.out.println();
		}
		for(int i=0;i<5;++i) //rows
		{   
			for(int j=0;j<5;++j) //columns
			{
				System.out.print("1");
			}
			System.out.println();
		}
		for(int i=0;i<5;++i) //rows
		{   
			for(int j=0;j<5;++j) //columns
			{
				System.out.print("*");
				
			}
			System.out.println();
			break;
		}
	}

}

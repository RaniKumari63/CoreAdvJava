package com.patterns;

public class Pattern11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=5;i++)
		{
	  for(int j=4;j>=i;j--)
	  { if(j<=4 && i)
		  System.out.print(" ");  
	   System.out.print("*");
	  
	  }
		System.out.println();  
	  
		}
	}

}

<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>LoginServlet</servlet-name>
  <display-name>LoginServlet</display-name>
  <servlet-class>com.suchiit.controller.LoginServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FoGotPasswordServlet</servlet-name>
  <display-name>FoGotPasswordServlet</display-name>
  <servlet-class>com.suchiit.controller.FoGotPasswordServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>RegistrationServlet</servlet-name>
  <display-name>RegistrationServlet</display-name>
  <servlet-class>com.suchiit.controller.RegistrationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>RegistrationServlet1</servlet-name>
  <display-name>RegistrationServlet1</display-name>
  <servlet-class>com.suchiit.controller.RegistrationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ProductRegistrationServlet</servlet-name>
  <display-name>ProductRegistrationServlet</display-name>
  <servlet-class>com.suchiit.controller.ProductRegistrationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ViewServlet</servlet-name>
  <display-name>ViewServlet</display-name>
  <servlet-class>com.suchiit.controller.ViewServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UpdateServlet</servlet-name>
  <display-name>UpdateServlet</display-name>
  <servlet-class>com.suchiit.controller.UpdateServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SubmitUpdateServlet</servlet-name>
  <display-name>SubmitUpdateServlet</display-name>
  <servlet-class>com.suchiit.controller.SubmitUpdateServlet</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>DeleteServlet</servlet-name>
 	<display-name>DeleteServlet</display-name>
 	<description></description>
 	<servlet-class>com.suchiit.controller.DeleteServlet</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>ViewSpecificServlet</servlet-name>
 	<display-name>ViewSpecificServlet</display-name>
 	<description></description>
 	<servlet-class>com.suchiit.controller.ViewSpecificServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FoGotPasswordServlet</servlet-name>
  <url-pattern>/forgot</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>RegistrationServlet</servlet-name>
  <url-pattern>/registration</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>RegistrationServlet1</servlet-name>
  <url-pattern>/registration1</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ProductRegistrationServlet</servlet-name>
  <url-pattern>/productregistration</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ViewServlet</servlet-name>
  <url-pattern>/view</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UpdateServlet</servlet-name>
  <url-pattern>/update</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SubmitUpdateServlet</servlet-name>
  <url-pattern>/submitupdate</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>DeleteServlet</servlet-name>
 	<url-pattern>/delete</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>ViewSpecificServlet</servlet-name>
 	<url-pattern>/viewone</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/Login1.html</welcome-file>
 </welcome-file-list>
</web-app>

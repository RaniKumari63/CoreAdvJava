package com.iterate;

import java.util.Scanner;

public class ForEx5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count = 0;
		for (int num = 2; num <= 100; ++num) {
		
				for (int i = 2; i < num; ++i) {
					if (num % i == 0) {
						System.out.println(num + "isnot prime number");
						count++;

					}
				}
                if(count==0) { System.out.println(num+"is prime number");
                }
                
			}

		}

	}



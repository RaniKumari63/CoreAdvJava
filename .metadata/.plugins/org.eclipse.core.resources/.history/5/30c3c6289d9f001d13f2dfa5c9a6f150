public class Test2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
List<String> l=new ArrayList<String>();
l.add("aaaaa");
l.add("bbbb");
l.add("cccc");
l.add("dddd");
l.add("eeee");
l.add("ffff");
l.add("gggg");
l.add("hhhh");
l.add("iiii");




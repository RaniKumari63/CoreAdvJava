package com.iterate;

import java.util.Scanner;

public class ForEx5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count = 0;
		for (int num = 0; num <= 100; ++num) {
			if ((num == 0) || (num == 1)) {
				System.out.println(num + "is not prime number");
			} else {
				for (int i = 2; i < num; ++i) {
					if (num % i == 0) {
						System.out.println(num + "isnot prime number");
						count++;

					}
				}
                if(count==0) { System.out.println("is a prime number");
                }
                }
			}

		}

	}



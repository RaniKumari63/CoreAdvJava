package fundamental;

public class SwapTWOT {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno=100;
		int sno=200;
		int eno=300;
		int gno=400;
		int temp=0;
		System.out.println("before swaping");
		System.out.println(fno+"\t"+sno+"\t"+eno+"\t"+gno);
		/*fno=fno+sno;
		sno=fno-sno;
		fno=fno-sno;
		sno=sno+eno;
		eno=sno-eno;
		sno=sno-eno;*/
		fno=fno+sno+eno+gno;
		sno=fno-sno-eno-gno;
		eno=fno-sno-eno-gno;
		fno=fno-sno-eno-gno;
		System.out.println(fno+"\t"+sno+"\t"+eno+"\t"+gno);
	}

}
